# ejercicios-resueltos

import string #Importamos las librerias
import random
num = int (input ("Ingrese el ancho de la contraseña"))
rangepass = string.ascii_letters+string.digits
def generar_contraseña():
         # El bit de función especifica la salida de 15 contraseñas aleatorias al tiempo que admite el uso definido por el usuario
    contraseña1 = ''
    for i in range(num):
        contraseña1 += random.choice(rangepass)
    return contraseña1
data = generar_contraseña()
print(data)



